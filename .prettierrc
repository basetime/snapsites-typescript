{
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "printWidth": 100,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrder": ["^@/(.*)$", "^[./]" ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
